<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="myMarking">
	<select id="list" resultType="Map" parameterType="Map" timeout="10">

	</select>

	<select id="read" resultType="Map" parameterType="Map">

	</select>
	
	<select id="listCount" resultType="int" parameterType="Map" timeout="10">
	SELECT COUNT(MARKING_KEY)
	FROM NB60344_MARKING
	WHERE 1=1
		AND MEMBER_KEY = #{MEMBER_KEY}
	</select>



</mapper>
